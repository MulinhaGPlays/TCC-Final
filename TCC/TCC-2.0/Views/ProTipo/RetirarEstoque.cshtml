@model  TCC_2._0.Models.PROTIPO


@{
    ViewBag.Title = "RetirarEstoque";
}

<link href="~/css/funcaoCrud.css" rel="stylesheet"/>

<div class="create">
    <form action="/ProTipo/RetirarEstoque?id=@Model.PTID" method="post">
        <div>
            <h2>Atualizando Estoque</h2>
            <br />
            <label for="qtd">Quantidade</label>
        </div>

        <div class="descricao-create">
            <class ="form-group">
                
                <input type="number" class="form-control"
                       id="qtd" name="qtd" min="0"
                    
                       required />
        </div>

        <div>

            <div>
                <button type="submit" class="btn btn-success">Salvar</button>
                <a href="/ProTipo/Alerta" class="btn btn-primary">Voltar</a>

            </div>

        </div>
    </form>
</div>
