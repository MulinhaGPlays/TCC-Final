@model  TCC_2._0.Models.PROTIPO


@{
    ViewBag.Title = "Estoque";
}

<h2>Atualizando Estoque</h2>

<div>
    <form action="/ProTipo/Estoque?id=@Model.PTID" method="post">


        <div>
            <class ="form-group">
                <label for="qtd">Quantidade</label>
                <input type="number" class="form-control"
                       id="qtd" name="qtd" min="0"
                    
                       required />
        </div>

       


        <div>



            <div>
                <button type="submit" class="btn btn-success">Salvar</button>
                <a href="/ProTipo/Index" class="btn btn-primary">Voltar</a>

            </div>


        </div>
    </form>
</div>
